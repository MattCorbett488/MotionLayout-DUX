<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.willowtree.matthewcorbett.motionlayoutdux">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="com.willowtree.matthewcorbett.motionlayoutdux.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.willowtree.matthewcorbett.motionlayoutdux.activities.FragmentTransitionActivity"
            android:label="@string/title_activity_fragment_transition" />
        <activity
            android:name="com.willowtree.matthewcorbett.motionlayoutdux.activities.AppBarLayoutActivity" />
        <activity android:name=".activities.ViewPagerActivity" />
        <activity android:name=".activities.KeyframeActivity" />
    </application>

</manifest>